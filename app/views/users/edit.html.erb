<h1>Users#edit</h1>
<p>Find me in app/views/users/edit.html.erb</p>
<%= form_for @user,url: users_update_path do |f| %>
  <%= f.label :name %>
  <p><%= f.text_field :name %></p>
  <p><%= f.label :email %></p>
  <p><%= f.text_field :email %></p>
  <p><%= f.label :password %></p>
  <p><%= f.text_field :password %></p>
  <p><%= f.label :password_confirmation %></p>
  <p><%= f.text_field :password_confirmation %></p>
  <p><%= f.submit %>
<% end %>